.iel-itms-prototype {
    text-align: center
}

.iel-itms-prototype {
    height: 500px;
    background-image: url("<%= current_page.data.prototypeImageURL %>");
    /*background-image: url("../images/iel-itms/DC/BG.jpg");*/
    background-position: center;
    margin: 0 -20px
}

.iel-itms-prototype h1 {
    color: #fff;
    font-size: 55px;
    font-weight: 600;
    margin-top: 130px;
    text-shadow: 1px 1px 0 rgba(0, 0, 0, .2)
}

.iel-itms-prototype h2 {
    color: #fff;
    font-size: 22px;
    font-weight: 400;
    margin-top: 30px;
    margin-bottom: 40px
}